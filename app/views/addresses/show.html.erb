<h1>Addresses#show</h1>
<h1><%= @location.name %> - <%= @address.street %></h1>
<p><%= @address.street %> <%= @address.state %></p>
<%= link_to 'Edit Address', edit_location_address_path([@location, @address]) %>
<%= link_to 'Delete Address', [@location, @address], method: :delete %>
<%= link_to 'Back To Addresses', location_addresses_path(@location) %>
